{
  "name": "dossr",
  "version": "0.0.8",
  "description": "sever side render",
  "main": "./index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "NODE_ENV=production node server/expressServer.js",
    "clean": "node sh/clean-dist.js",
    "compile": "npx babel src --out-dir dist",
    "copy": "node sh/copy-dist.js",
    "package": "npm run clean & npm run compile & npm run copy",
    "publish": "npm publish dist",
    "package-copy-lib": "npx babel lib --out-dir example/standard/node_modules/dossr/lib --copy-files",
    "package-copy-files": "npx babel index.js net.js server.js package.json --out-dir example/standard/node_modules/dossr --copy-files",
    "package-copy": "npm run package-copy-lib & npm run package-copy-files",
    "package-build": "npm run package & npm run package-copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palmg/dossr.git"
  },
  "keywords": [
    "palmg", "react", "nextjs", "ssr", "server side render", "support ie6~8"
  ],
  "author": "palmg",
  "license": "MIT",
  "bin": {
    "create-dossr": "./bin/create-app.js",
    "create-dossr-blank": "./bin/create-blank-app.js",
    "create-dossr-antd": "./bin/create-antd-app.js"
  },
  "bugs": {
    "url": "https://github.com/palmg/dossr/issues"
  },
  "homepage": "https://github.com/palmg/dossr#readme",
  "dependencies": {
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "ua-parser-js": "^0.7.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "copyfiles": "^2.1.0",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "peerDependencies": {
    "next": "^7.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
